{
  "name": "winn-pro-show-app",
  "version": "1.0.0",
  "description": "Winn-Pro Show event app for vendors and attendees",
  "scripts": {
    "build:css": "npx tailwindcss -i ./css/tailwind-input.css -o ./css/tailwind.css --minify",
    "watch:css": "npx tailwindcss -i ./css/tailwind-input.css -o ./css/tailwind.css --watch",
    "dev": "npx tailwindcss -i ./css/tailwind-input.css -o ./css/tailwind.css --watch",
    "build": "npm run build:css",
    "cap:init": "npx cap init 'Winn-Pro Show' app.winnpro.mobile --web-dir .",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:run:android": "npx cap run android",
    "cap:run:ios": "npx cap run ios",
    "mobile:build": "npm run build:css && npx cap sync"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "@capacitor/core": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/android": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/toast": "^6.0.0",
    "@capacitor/push-notifications": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor-community/barcode-scanner": "^4.0.0",
    "stripe": "^16.6.0"
  },
  "keywords": ["home-show", "event-app", "vendors", "mobile", "capacitor"],
  "author": "Andrew Loftis",
  "license": "MIT"
}
